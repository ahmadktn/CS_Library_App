{
  "expo": {
    "name": "CS_Library_App",
    "slug": "umyu-library",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rareplayersorganization.umyulibrary"
    },
    "android": {
      "permissions": [
        "WRITE_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.rareplayersorganization.umyulibrary"
    },
    "web": {
      "favicon": "./assets/icon.jpg"
    },
    "extra": {
      "eas": {
        "projectId": "182e0d81-8608-415f-88b8-1d1d75c03457"
      }
    },
    "plugins": [
        [
          "expo-build-properties",
          {
           "android": {
              "usesCleartextTraffic": true
            }
          }
        ]
      ],
    "owner": "rareplayers-organization"
  }
}
